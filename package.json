{
  "name": "social-api-import",
  "version": "0.1.0",
  "author": {
    "name": "Mark Kennedy",
    "url": "http://heyimmark.com"
  },
  "license": "MIT",
  "description": "Dynamically load any social network API using JavaScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/mkay581/social-api-import.git"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "keywords": [
    "social network import",
    "social importer",
    "social media importer",
    "social media script loader",
    "script loader",
    "social",
    "social network api",
    "social api",
    "social networks",
    "sdk",
    "facebook sdk",
    "twitter api",
    "tumblr api",
    "instagram api",
    "facebook api",
    "vine api"
  ],
  "main": "dist/index.js",
  "types": "src/index.ts",
  "scripts": {
    "test": "karma start karma.conf.js && tslint src/**/*",
    "test-server": "karma start karma.conf.js --single-run=false",
    "preversion": "npm test",
    "minify": "uglifyjs dist/scroll.js -o dist/scroll.min.js",
    "build-dist": "rm -rf dist/* && rollup -c && npm run minify",
    "version": "npm run build-dist && banner-cli dist/*.js && git add -A dist",
    "postversion": "git push && git push --tags",
    "start-dev": "rollup -c --watch"
  },
  "devDependencies": {
    "banner-cli": "0.11.1",
    "chai": "^4.1.2",
    "es6-promise": "^4.0.5",
    "eslint": "^5.5.0",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-rollup-preprocessor": "^6.0.1",
    "mocha": "^5.2.0",
    "mock-raf": "^1.0.0",
    "rollup": "^0.67.0",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-typescript": "^1.0.0",
    "sinon": "^7.1.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.3",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "dynamic-import": "^0.1.0",
    "oauth": "^0.9.14"
  }
}
